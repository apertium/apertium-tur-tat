      <program name="apertium-postchunk">
        <file name="apertium-tr-tt.tr-tt.t3x"/>
        <file name="tr-tt.t3x.bin"/>
      </program>
      <program name="hfst-proc $1">
        <file name="tr-tt.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>

